{
    "name": "qobo/cakephp-utils",
    "description": "CakePHP utilities",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Qobo Ltd",
            "email": "info@qobo.biz",
            "homepage": "https://www.qobo.biz",
            "role": "Developer"
        }
    ],
    "require": {
        "admad/cakephp-jwt-auth": "^2.0",
        "alt3/cakephp-swagger": "^1.0",
        "burzum/cakephp-file-storage": "^1.2",
        "burzum/cakephp-imagine-plugin": "^2.1",
        "cakedc/users": "^4.0",
        "cakephp/cakephp": "^3.4.0 <3.5.0",
        "cakephp/migrations": "~1.0",
        "doctrine/annotations": "v1.4.0",
        "doctrine/instantiator": "1.0.5",
        "friendsofcake/crud": "^4.3",
        "fzaninotto/faker": "^1.0",
        "league/csv": "^8.1",
        "league/json-guard": "^0.5",
        "lorenzo/audit-stash": "^1.0",
        "maiconpinto/cakephp-adminlte-theme": "^1.0",
        "muffin/trash": "^1.1",
        "myclabs/php-enum": "^1.5",
        "piwik/ini": "^1.0",
        "pyrech/composer-changelogs": "^1.4",
        "zircote/swagger-php": "2.0.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "cakephp/cakephp-codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Qobo\\Utils\\": "src",
            "Qobo\\Utils\\Test\\Fixture\\": "tests\\Fixture"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qobo\\Utils\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
