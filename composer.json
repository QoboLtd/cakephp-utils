{
    "name": "qobo/cakephp-utils",
    "description": "CakePHP utilities",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Qobo Ltd",
            "email": "info@qobo.biz",
            "homepage": "https://www.qobo.biz",
            "role": "Developer"
        }
    ],
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.1"
        }
    },
    "require": {
        "admad/cakephp-jwt-auth": "^2.0",
        "admad/cakephp-sequence": "^2.2",
        "alt3/cakephp-swagger": "^2.0",
        "arvenil/ninja-mutex": "^0.6",
        "burzum/cakephp-file-storage": "^1.2",
        "burzum/cakephp-imagine-plugin": "^2.1",
        "cakedc/users": "^6.0",
        "cakephp/cakephp": "^3.5",
        "cakephp/migrations": "^1.7",
        "friendsofcake/crud": "^5.4",
        "fzaninotto/faker": "^1.0",
        "league/csv": "^8.1",
        "league/json-guard": "^0.5",
        "lorenzo/audit-stash": "^1.0",
        "maiconpinto/cakephp-adminlte-theme": "^1.0",
        "muffin/trash": "^1.1",
        "myclabs/php-enum": "^1.5",
        "pyrech/composer-changelogs": "^1.4",
        "riesenia/cakephp-duplicatable": "^3.0",
        "rlanvin/php-rrule": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "cakephp/cakephp-codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Qobo\\Utils\\": "src",
            "Qobo\\Utils\\Test\\Fixture\\": "tests\\Fixture"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qobo\\Utils\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
