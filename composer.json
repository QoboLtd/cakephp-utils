{
    "name": "qobo/cakephp-utils",
    "description": "CakePHP utilities",
    "type": "cakephp-plugin",
    "license": "GPL-2.0",
    "require": {
        "cakephp/cakephp": ">3.0.0 <4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "cakephp/cakephp-codesniffer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Qobo\\Utils\\": "src",
            "Qobo\\Utils\\Test\\Fixture\\": "tests\\Fixture"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qobo\\Utils\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
